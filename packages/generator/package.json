{
  "name": "prisma-generator-proto",
  "description": "Generate proto files from prisma schema",
  "version": "0.1.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-proto": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc && cp -r src/template dist",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@prisma/generator-helper": "3.12.0",
    "@prisma/internals": "^4.10.1",
    "@prisma/sdk": "3.12.0",
    "change-case": "^4.1.2",
    "prettier": "2.5.1",
    "underscore": "^1.13.6",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/change-case": "^2.3.1",
    "@types/jest": "27.0.3",
    "@types/node": "18.15.2",
    "@types/prettier": "2.4.2",
    "@types/underscore": "^1.11.4",
    "jest": "27.4.7",
    "prisma": "3.12.0",
    "semantic-release": "^18.0.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.2"
  },
  "homepage": "Link to homepage or github readme here",
  "repository": {
    "type": "git",
    "url": "Repo link here"
  },
  "author": "Your Name <Your Email>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}